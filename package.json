  "files": [
    "bin",
    "cli-dist",
    "vun-dist"
  ],
  "bin": {
    "vun": "bin/vun.js"
  },
  "scripts": {
    "cleanup:cli": "rm -rf ./cli-dist ./.cache/cli.tsbuildinfo",
    "cleanup:vun": "rm -rf ./vun-dist ./.cache/vun.tsbuildinfo",
    "cleanup": "npm run cleanup:cli && npm run cleanup:vun && rm -rf ./.cache",
    "dev:cli": "ttsc -p ./tsconfig.cli.json -w",
    "dev:vun": "tsc -p ./tsconfig.vun.json -w",
    "dev": "npm run dev:cli & npm run dev:vun",
    "build:cli:prod": "npm run cleanup:cli && ttsc -p ./tsconfig.cli.json",
    "build:vun:prod": "npm run cleanup:vun && tsc -p ./tsconfig.vun.json",
    "build": "npm run build:cli:prod && npm run build:vun:prod",
    "lint": "eslint --ext .ts cli vuniversal",
    "test": "jest tests",
    "test:watch": "jest tests --watch -i",
    "rebirth": "npm run lint && npm run build",
    "release": ". ./scripts/release.sh"
  },
